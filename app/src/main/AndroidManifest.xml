<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sqlite_program">

    <application
        android:allowBackup="true"
        android:icon="@drawable/note"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:windowSoftInputMode="stateAlwaysHidden"
        android:supportsRtl="true"
        android:theme="@style/MyLightTheme"
        android:fullBackupContent="true" >
        <activity
            android:name=".activity.Intro_Activity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.New_Activity" />
        <activity android:name=".activity.Main_Activity"/>
        <activity android:name=".activity.Setting_Activity"/>
        <activity android:name=".activity.Password_Activity"/>
        <activity android:name=".activity.Tutorial_Activity"></activity>
        <activity android:name=".activity.NewSetting_Activity"></activity>
        <activity
            android:name=".activity.old_password_Activity"
            android:exported="true">
            <intent-filter>
                <action
                    android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
    </application>

</manifest>